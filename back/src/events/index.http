@apiUrl = http://localhost:3001

# Eventos do dia atual
GET {{apiUrl}}/api/events?dateFilter=day
X-Site-Key: sk_5xjk338oj46hcvdqmsj2gfqh

# Eventos da semana com paginação
GET {{apiUrl}}/api/events?dateFilter=week&limit=50&offset=0
X-Site-Key: sk_5xjk338oj46hcvdqmsj2gfqh

# Filtrar por nome de evento
GET {{apiUrl}}/api/events?name=page_view&dateFilter=month
X-Site-Key: sk_5xjk338oj46hcvdqmsj2gfqh

# Período customizado
GET {{apiUrl}}/api/events?dateFilter=custom&startDate=2024-01-01T00:00:00Z&endDate=2024-01-31T23:59:59Z
X-Site-Key: sk_5xjk338oj46hcvdqmsj2gfqh
